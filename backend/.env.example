# Wayo Backend Environment Variables

# Rails environment (development, test, production)
RAILS_ENV=development

# Database Configuration
# DATABASE_URL=postgres://user:password@hostname:5432/db_name

# Frontend URL for CORS (no trailing slash)
# For local development: http://localhost:3400
# For production: https://wayo.co.kr
FRONTEND_URL=http://localhost:3400

# Subdomain support (if applicable)
ALLOWED_HOSTS=localhost,wayo.co.kr,gabojago.wayo.co.kr
